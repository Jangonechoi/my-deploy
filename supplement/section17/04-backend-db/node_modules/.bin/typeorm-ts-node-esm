#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/choi/Desktop/frontend-react/supplement/section17/04-backend-db/node_modules/.pnpm/typeorm@0.3.26_pg@8.16.3_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@24.5.2_typescript@5.9.2_/node_modules/typeorm/node_modules:/Users/choi/Desktop/frontend-react/supplement/section17/04-backend-db/node_modules/.pnpm/typeorm@0.3.26_pg@8.16.3_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@24.5.2_typescript@5.9.2_/node_modules:/Users/choi/Desktop/frontend-react/supplement/section17/04-backend-db/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/choi/Desktop/frontend-react/supplement/section17/04-backend-db/node_modules/.pnpm/typeorm@0.3.26_pg@8.16.3_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@24.5.2_typescript@5.9.2_/node_modules/typeorm/node_modules:/Users/choi/Desktop/frontend-react/supplement/section17/04-backend-db/node_modules/.pnpm/typeorm@0.3.26_pg@8.16.3_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@24.5.2_typescript@5.9.2_/node_modules:/Users/choi/Desktop/frontend-react/supplement/section17/04-backend-db/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/typeorm@0.3.26_pg@8.16.3_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@24.5.2_typescript@5.9.2_/node_modules/typeorm/cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../.pnpm/typeorm@0.3.26_pg@8.16.3_reflect-metadata@0.2.2_ts-node@10.9.2_@types+node@24.5.2_typescript@5.9.2_/node_modules/typeorm/cli-ts-node-esm.js" "$@"
fi
